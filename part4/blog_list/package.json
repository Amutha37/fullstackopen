{
  "name": "blog",
  "version": "1.0.0",
  "description": "\"list favourite blog with likes rating\"",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "build:ui": "rm -rf build && cd ../bloglist-frontend/ && npm run build && cp -r build ../blog_list",
    "deploy": "git push heroku main",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "start:test": "cross-env NODE_ENV=test node index.js"
  },
  "repository": {
    "type": "git",
    "url": "\"https://github.com/Amutha37/fullstackopen/tree/main/part4/blog_list\""
  },
  "keywords": [
    "blog"
  ],
  "author": "Amutha Muhunthan",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "install-peers": "^1.0.4",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.13.15",
    "mongoose-unique-validator": "^2.0.3"
  }
}
